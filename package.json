{
  "name": "coworking.party",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "concurrently -c auto -t 'HH:mm:ss.SSS' -p '[{name} {time}]' pnpm:dev:*",
    "dev-lite": "concurrently -c auto -t 'HH:mm:ss.SSS' -p '[{name} {time}]' pnpm:dev:next pnpm:dev:db:turso",
    "dev-nosst": "concurrently -c auto -t 'HH:mm:ss.SSS' -p '[{name} {time}]' pnpm:dev-next pnpm:dev:db:turso pnpm:dev:db:studio pnpm:dev:partykit",
    "dev-next": "next dev",
    "dev-nodb": "concurrently -c auto -t 'HH:mm:ss.SSS' -p '[{name} {time}]' pnpm:dev:next pnpm:dev:partykit",
    "dev:next": "sst dev next dev",
    "dev:partykit": "partykit dev --live",
    "db": "concurrently pnpm:dev:db:*",
    "dev:db:turso": "turso dev --db-file ./db/dev.db",
    "dev:db:studio": "pnpm run db:studio",
    "pbs": "pnpm run build && pnpm run start",
    "build": "next build",
    "build:local": "dotenv -e .env -- next build",
    "start": "next start",
    "start:local": "concurrently pnpm:start pnpm:db:*",
    "lint": "next lint",
    "check": "tsc --noEmit",
    "deploy": "pnpm run deploy:preview",
    "deploy:preview": "pnpm run db:dump:preview && pnpm run db:dump:prune:preview && pnpm run deploy:preview:partykit && pnpm run deploy:preview:site",
    "deploy:preview:partykit": "cp .env .env.bak; cp .env.preview .env; partykit deploy --preview preview --with-vars; mv .env.bak .env",
    "deploy:preview:site": "dotenv -e .env.preview -- sst deploy --stage preview",
    "deploy:prod": "pnpm run db:dump:prod && pnpm run db:dump:prune:prod && pnpm run deploy:prod:partykit && pnpm run deploy:prod:site",
    "deploy:prod:partykit": "cp .env .env.bak; cp .env.production .env; partykit deploy --with-vars; mv .env.bak .env",
    "deploy:prod:site": "dotenv -e .env.production -- sst deploy --stage production",
    "db:push": "drizzle-kit push",
    "db:push:prod": "dotenv -e .env.production -- drizzle-kit push",
    "db:push:preview": "dotenv -e .env.preview -- drizzle-kit push",
    "db:studio": "drizzle-kit studio",
    "db:shell": "turso db shell http://127.0.0.1:8080",
    "db:shell:preview": "turso db shell coworking-party-preview",
    "db:shell:prod": "turso db shell coworking-party",
    "db:dump": "turso db shell http://127.0.0.1:8080 .dump > db/exports/coworking_party_local_`date -u +\"%Y-%m-%dT%H-%M-%S\"`.sql",
    "db:dump:preview": "turso db shell coworking-party-preview .dump > db/exports/coworking_party_preview_`date -u +\"%Y-%m-%dT%H-%M-%S\"`.sql",
    "db:dump:prod": "turso db shell coworking-party .dump > db/exports/coworking_party_prod_`date -u +\"%Y-%m-%dT%H-%M-%S\"`.sql",
    "db:dump:prune": "find db/exports -name \"coworking_party_local*sql\" -type f -mtime 14 -exec rm -f {} +",
    "db:dump:prune:preview": "find db/exports -name \"coworking_party_preview*sql\" -type f -mtime 14 -exec rm -f {} +",
    "db:dump:prune:prod": "find db/exports -name \"coworking_party_prod*sql\" -type f -mtime 14 -exec rm -f {} +"
  },
  "dependencies": {
    "@auth/drizzle-adapter": "1.1.0",
    "@dnd-kit/core": "^6.1.0",
    "@dnd-kit/sortable": "^8.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@fontsource/space-grotesk": "^5.0.18",
    "@hookform/resolvers": "^3.4.0",
    "@libsql/client": "^0.6.0",
    "@next/third-parties": "^14.2.3",
    "@radix-ui/react-avatar": "^1.0.4",
    "@radix-ui/react-checkbox": "^1.0.4",
    "@radix-ui/react-collapsible": "^1.0.3",
    "@radix-ui/react-dialog": "^1.0.5",
    "@radix-ui/react-dropdown-menu": "^2.0.6",
    "@radix-ui/react-icons": "^1.3.0",
    "@radix-ui/react-label": "^2.0.2",
    "@radix-ui/react-popover": "^1.0.7",
    "@radix-ui/react-progress": "^1.0.3",
    "@radix-ui/react-select": "^2.0.0",
    "@radix-ui/react-slot": "^1.0.2",
    "@radix-ui/react-tooltip": "^1.0.7",
    "@sentry/nextjs": "^8.2.1",
    "@tabler/icons-react": "^3.4.0",
    "boolean": "^3.2.0",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "cmdk": "^1.0.0",
    "dotenv": "^16.4.5",
    "drizzle-orm": "^0.30.10",
    "drizzle-zod": "^0.5.1",
    "framer-motion": "^11.2.4",
    "humanize-duration": "^3.32.0",
    "jotai": "^2.8.0",
    "linkify-react": "^4.1.3",
    "lodash": "^4.17.21",
    "next": "14.2.3",
    "next-auth": "5.0.0-beta.18",
    "partysocket": "1.0.1",
    "pluralize": "^8.0.0",
    "posthog-js": "^1.131.4",
    "react": "^18.3.1",
    "react-canvas-confetti": "^2.0.7",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.51.4",
    "react-hotkeys-hook": "^4.5.0",
    "sonner": "^1.4.41",
    "tailwind-merge": "^2.3.0",
    "tailwindcss-animate": "^1.0.7",
    "use-sound": "^4.0.1",
    "usehooks-ts": "^3.1.0",
    "uuid": "^9.0.1",
    "zod": "^3.23.8",
    "zod-validation-error": "^3.3.0",
    "zustand": "^4.5.2"
  },
  "devDependencies": {
    "@types/humanize-duration": "^3.27.4",
    "@types/lodash": "^4.17.4",
    "@types/node": "^20.12.12",
    "@types/pluralize": "^0.0.33",
    "@types/react": "^18.3.2",
    "@types/react-dom": "^18.3.0",
    "@types/uuid": "^9.0.8",
    "concurrently": "^8.2.2",
    "dotenv-cli": "^7.4.2",
    "drizzle-kit": "^0.21.2",
    "eslint": "^8",
    "eslint-config-next": "14.2.3",
    "partykit": "0.0.104",
    "postcss": "^8.4.38",
    "sst": "^3.0.15",
    "tailwindcss": "^3.4.3",
    "typescript": "^5.4.5"
  },
  "cspell": {
    "ignorePaths": [
      "pnpm-lock.yaml"
    ]
  }
}
